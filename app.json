{
  "name": "Discord Downtime Notifier",
  "description": "A small Discord bot that sends a message when a specified bot goes offline.",
  "repository": "https://github.com/Akhil1718/DownBot-NotiFirer",
  "logo": "https://cdn.discordapp.com/attachments/712911895140761600/721187762765824020/android-q-notification-inbox-100785464-large.jpg",
  "env": {
    "PREFIX": {
      "description": "Your bot's command prefix.",
      "value": "d!",
      "required": true
    },
    "DISCORD_TOKEN": {
      "description": "Your bot's bot token.",
      "required": true
    }
  },
  "addons": ["mongolab"]
}
